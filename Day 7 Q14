#include <stdio.h>

int main() {
    int terms;
    int num1 = 0, num2 = 1, nextNum;

    printf("How many Fibonacci numbers do you want? ");
    scanf("%d", &terms);

    printf("Fibonacci Series: ");

    for(int i = 1; i <= terms; i++) 
    {    
        nextNum = num1 + num2;
        num1 = num2;
        num2 = nextNum;
    }
    printf("%d ", num1);

    return 0;
}
